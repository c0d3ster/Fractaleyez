{
  "name": "audio-visualization-starter",
  "version": "0.0.1",
  "description": "Fractaleyez Audio Visualizer",
  "main": "app.js",
  "scripts": {
    "build:dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "start": "concurrently \"npm run build\" \"webpack-dev-server --mode production --open\"",
    "start:dev": "concurrently --kill-others \"npm run build:dev\" \"webpack-dev-server --mode development --open\""
  },
  "author": "Cody Douglass",
  "license": "ISC",
  "dependencies": {
    "three": "^0.90.0",
    "dat.gui": "^0.7.1",
    "stats-js": "^1.0.0-alpha1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^3.5.1",
    "webpack": "^4.0.0",
    "webpack-cli": "^2.0.8",
    "webpack-dev-server": "^3.0.0-beta.2"
  }
}
