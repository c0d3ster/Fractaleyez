{
  "name": "ReactNodeBoilerplate",
  "version": "1.0.0",
  "description": "React, React Router, Express, and Webpack",
  "main": "server.js",
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "start": "concurrently --kill-others \"npm run build\" \"node server.js\"",
    "start-dev": "concurrently --kill-others \"npm run dev\" \"supervisor server.js\""
  },
  "author": "Cody Douglass",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.9",
    "json-loader": "^0.5.4",
    "file-loader": "^1.1.6",
    "postcss-loader": "^2.1.0",
    "style-loader": "^0.20.2",
    "supervisor": "^0.12.0",
    "url-loader": "^0.6.2",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.9"
  }
}
