{
  "name": "fractaleyez",
  "version": "1.0.0",
  "description": "Fractaleyez - A Real-Time Audio Visualizer",
  "main": "app.js",
  "scripts": {
    "build-dev": "webpack --mode development --watch",
    "client-dev": "webpack-dev-server --mode development --content-base public/",
    "server-dev": "node -r esm server-dev.js",
    "build": "webpack --mode production",
    "start": "yarn build && supervisor server.js",
    "start-dev": "concurrently --kill-others \"yarn server-dev\" \"yarn client-dev\"",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d public",
    "lint": "eslint src --fix"
  },
  "author": "Cody Douglass",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "axios": "^0.21.2",
    "bootstrap": "^3.4.1",
    "classnames": "^2.2.6",
    "esm": "^3.2.25",
    "mongoose": "^5.7.5",
    "nodemon": "^1.18.9",
    "postprocessing": "^5.0.0",
    "react": "^16.6.3",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.6.0",
    "stats-js": "^1.0.0-alpha1",
    "three": "^0.137.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.4",
    "concurrently": "^4.0.1",
    "css-loader": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jsdoc": "^22.1.0",
    "eslint-plugin-prefer-arrow": "^1.1.7",
    "eslint-scope": "^5.0.0",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "gh-pages": "^2.2.0",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "react-dev-utils": "^5.0.2",
    "style-loader": "^0.23.0",
    "supervisor": "^0.12.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c0d3ster/Fractaleyez.git"
